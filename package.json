{
  "name": "dealver",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:local": "cross-env NODE_ENV=local nest build --webpack --webpackPath webpack-hmr.config.js --watch",
    "start:dev": "cross-env NODE_ENV=dev nest build --webpack --webpackPath webpack-hmr.config.js --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/axios": "^3.0.0",
    "@nestjs/common": "9.0.5",
    "@nestjs/config": "2.3.4",
    "@nestjs/core": "9.0.5",
    "@nestjs/passport": "9.0.3",
    "@nestjs/platform-express": "9.0.0",
    "@nestjs/swagger": "6.3.0",
    "@nestjs/typeorm": "9.0.1",
    "@types/pino": "7.0.5",
    "axios": "^1.4.0",
    "bcryptjs": "2.4.3",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "cookie-parser": "1.4.6",
    "cross-env": "7.0.3",
    "ioredis": "^5.3.2",
    "joi": "17.9.2",
    "jsonwebtoken": "9.0.0",
    "mysql2": "3.3.5",
    "passport": "0.6.0",
    "passport-jwt": "4.0.1",
    "passport-local": "1.0.0",
    "pino": "8.14.1",
    "pino-pretty": "10.0.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.2.0",
    "typeorm": "0.3.16",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@golevelup/ts-jest": "0.3.7",
    "@nestjs/cli": "9.0.0",
    "@nestjs/schematics": "9.0.0",
    "@nestjs/testing": "9.0.0",
    "@types/express": "4.17.13",
    "@types/jest": "29.5.1",
    "@types/jsonwebtoken": "9.0.2",
    "@types/node": "18.16.12",
    "@types/passport-local": "1.0.35",
    "@types/supertest": "2.0.11",
    "@typescript-eslint/eslint-plugin": "5.0.0",
    "@typescript-eslint/parser": "5.0.0",
    "eslint": "8.42.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "jest": "29.5.0",
    "prettier": "2.3.2",
    "run-script-webpack-plugin": "0.2.0",
    "source-map-support": "0.5.20",
    "supertest": "6.1.3",
    "ts-jest": "29.1.0",
    "ts-loader": "9.2.3",
    "tsconfig-paths": "4.2.0",
    "typescript": "4.3.5",
    "webpack": "5.86.0",
    "webpack-node-externals": "3.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
